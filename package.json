{
    "name": "myblog",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "material-design-icons-iconfont": "^4.0.3",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue": "^2.5.17",
        "vue-material": "^1.0.0-beta-10.2",
        "vue-router": "^3.0.2",
        "vuetify": "^1.3.14",
        "vuex": "^3.0.1",
        "vuex-persistedstate": "^2.5.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.2.0",
        "@vue/cli-plugin-eslint": "^3.2.0",
        "@vue/cli-service": "^3.2.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0-0",
        "vue-template-compiler": "^2.5.17"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-eval": 1,
            "no-lone-blocks": 2,
            "no-func-assign": 2,
            "no-alert": 0,
            "no-const-assign": 2,
            "no-constant-condition": 2,
            "no-debugger": 2,
            "no-dupe-args": 2,
            "no-else-return": 2,
            "no-empty": 2,
            "no-extra-semi": 2,
            "no-implicit-coercion": 1,
            "no-invalid-this": 2,
            "no-irregular-whitespace": 2,
            "no-mixed-spaces-and-tabs": [
                2,
                false
            ],
            "no-multi-spaces": 1,
            "no-multi-str": 2,
            "no-multiple-empty-lines": [
                1,
                {
                    "max": 2
                }
            ],
            "no-new-object": 2,
            "no-new-func": 1,
            "no-redeclare": 2,
            "no-regex-spaces": 2,
            "no-restricted-modules": 0,
            "no-return-assign": 1,
            "no-script-url": 0,
            "no-self-compare": 2,
            "no-trailing-spaces": 1,
            "no-undef": 1,
            "no-undef-init": 2,
            "no-undefined": 2,
            "no-unreachable": 2,
            "valid-typeof": 2,
            "no-unused-expressions": 2,
            "no-unused-vars": [
                2,
                {
                    "vars": "all",
                    "args": "after-used"
                }
            ],
            "no-use-before-define": 2,
            "no-useless-call": 2,
            "no-var": 0,
            "array-bracket-spacing": [
                2,
                "never"
            ],
            "arrow-parens": 0,
            "accessor-pairs": 0,
            "camelcase": 2,
            "consistent-this": [
                2,
                "that"
            ],
            "curly": [
                2,
                "all"
            ],
            "init-declarations": 0,
            "semi": [
                2,
                "always"
            ],
            "use-isnan": 2,
            "strict": 2,
            "spaced-comment": [
                "error",
                "always",
                {
                    "line": {
                        "markers": [
                            "/"
                        ],
                        "exceptions": [
                            "-",
                            "+"
                        ]
                    },
                    "block": {
                        "markers": [
                            "!"
                        ],
                        "exceptions": [
                            "*"
                        ],
                        "balanced": true
                    }
                }
            ],
            "eqeqeq": [
                2,
                "allow-null"
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
